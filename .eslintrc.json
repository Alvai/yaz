{
    "extends": ["airbnb-base"],
    "globals": {
        "assert": true
    },
    "env": {
        "browser": true,
        "node": true,
        "mocha": true,
        "es6": true
    },
    "rules": {
        "import/no-dynamic-require": 0,
        "arrow-body-style": 0,
        "prefer-destructuring":0,
        "class-methods-use-this":0,
        "no-console":0
    }
}
